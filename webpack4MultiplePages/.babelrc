{
  "presets": [
    ["@babel/preset-env",
      {
        "useBuiltIns": "usage",
      }
    ],
    "@babel/preset-react",
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ]
  ]
}